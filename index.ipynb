    "def climbing_stairs(n):\n",
    "    if n==1:\n",
    "        return 1\n",
    "    elif n==2:\n",
    "        return 2\n",
    "    a,b = 1,2\n",
    "    for i in range(3, n+1):\n",
    "        current_step = a+b\n",
    "        a = b\n",
    "        b = current_step\n",
    "    return b\n",
    "print(climbing_stairs(5))"
   ]
